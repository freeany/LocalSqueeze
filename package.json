    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "6.30.1",
    "react-router-dom": "^6.30.1",
    "scheduler": "^0.26.0",
    "set-cookie-parser": "^2.7.1",
    "sharp": "^0.34.3",
    "tailwind-merge": "^3.3.1"
  }
